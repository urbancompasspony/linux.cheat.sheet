SUDO
    (sudo crontab -l 2>/dev/null; echo "# Nextcloud") | sudo crontab -
    (sudo crontab -l 2>/dev/null; echo "*/5 * * * * docker exec -t -u www-data $NOMECONTAINER php -f /var/www/html/cron.php") | sudo crontab -

NON SUDO
    (crontab -l 2>/dev/null; echo "#*/15 * * * * docker exec -t -u www-data $NOMECONTAINER ./occ files:scan --unscanned --all") | crontab -
